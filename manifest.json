{
  "name": "traQ Spotify O2i",
  "description": "Spotify OGP in traQ to iframe element",
  "version": "0.2.0",
  "manifest_version": 2,
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "64": "icons/icon_64.png",
    "128": "icons/icon_128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://q.trap.jp/*"],
      "js": ["content_script.js"], "css": ["content_style.css"]
    }
  ]
}